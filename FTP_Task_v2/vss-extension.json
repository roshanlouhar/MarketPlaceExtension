{
	"manifestVersion": 1,
	"id": "CMCFileUpload",
	"version": "1.0.3",
	"name": "CMC File Uploader",
	"description": "A VSTS Build Task that will upload a file or folder to a destination in Azure using Published Profile Settings.",
	"publisher": "CampusManagementCorp",
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
			}
		],
	"icons": {
		"default": "icon.png"
	 },
	"contributions": [
		{
			"id": "CMCFileUploadBuildTask",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "buildtask"
			}
		}
	],
	"files":[
		{
			"path": "buildtask"			
		}
	]
}